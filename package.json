{
  "name": "synthetics-recorder",
  "version": "1.4.14",
  "description": "Record synthetics user journey flow of a website",
  "repository": "https://github.com/elastic/synthetics-recorder",
  "scripts": {
    "serve": "webpack serve --config webpack.config.js",
    "bundle": "webpack --config webpack.config.js",
    "analyze-bundle": "LAUNCH_ANALYZER=true webpack --config webpack.config.js",
    "install:pw": "node scripts/install-pw.js",
    "postinstall": "npm run install:pw && electron-builder install-app-deps && node dev-tools/dep-info.js && npm run prepare-demo-app --legacy-peer-deps",
    "prepare-demo-app": "npm install --prefix demo-app && npm run build --prefix demo-app",
    "dev": "concurrently -k \"npm run serve\" \"npm run tsc-electron -- -w\" \"wait-on --http-get-status-code=200 http://127.0.0.1:3000 && nodemon --exec electron build/electron/electron.js\"",
    "build": "NODE_ENV=production npm run bundle && npm run tsc-electron",
    "tsc-electron": "tsc -p tsconfig.node.json",
    "pack": "npm run build && electron-builder",
    "release": "npm run pack -- -mwl --publish=always",
    "release-ci": "npm run release-ci_mac-x64 && npm run release-ci_mac-arm64 && npm run release-ci_linux-x64 && npm run release-ci_windows-x64",
    "release-ci_mac-x64": "npm run pack -- --mac --x64 --config=electron-builder.mac-x64.yml --publish=never",
    "release-ci_mac-arm64": "npm run pack -- --mac --arm64 --config=electron-builder.mac-arm64.yml --publish=never",
    "release-ci_linux-x64": "npm run pack -- --linux --x64 --config=electron-builder.linux-x64.yml --publish=never",
    "release-ci_windows-x64": "npm run pack -- --windows --x64 --config=electron-builder.win-x64.yml --publish=never",
    "clean": "rm -rf ./build && rm -rf ./dist",
    "lint": "DEBUG=eslint:cli-engine eslint .",
    "format": "prettier --write .",
    "unused-exports": "node dev-tools/find-unused-exports.js",
    "lint:fix": "npm run lint -- --fix",
    "prepare": "husky install",
    "test": "npm run test:unit && npm run test:e2e",
    "test:unit": "jest --config ./jest.unit.config.js .",
    "test:unit:watch": "npm run test:unit -- --coverage --watch --collectCoverageFrom={src,electron}/**/*.{js,jsx,ts,tsx}",
    "test:e2e": "jest --runInBand --config ./jest.e2e.config.js ./e2e/tests",
    "test:e2e:server": "PORT=61337 npm run serve",
    "test:e2e:runner": "IS_RUNNER=true TEST_PORT=61337 jest --runInBand --config ./jest.e2e.config.js ./e2e/tests",
    "update-demo-app": "git subtree pull --prefix demo-app git@github.com:vigneshshanmugam/synthetics-ecommerce-demo.git main --squash"
  },
  "author": {
    "name": "Elastic Synthetics Team",
    "email": "synthrum@elastic.co"
  },
  "keywords": [
    "synthetics",
    "recorder",
    "elastic",
    "playwright"
  ],
  "license": "MIT",
  "main": "build/electron/electron.js",
  "homepage": "./",
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "npm run lint:fix",
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@babel/preset-env": "^7.27.2",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@electron/notarize": "^1.2.3",
    "@emotion/babel-plugin": "^11.13.5",
    "@emotion/babel-preset-css-prop": "^11.12.0",
    "@emotion/eslint-plugin": "^11.12.0",
    "@testing-library/react": "^12.1.4",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^17.0.0",
    "@types/react-dom": "^17.0.17",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "ajv": "^8.11.0",
    "babel-loader": "^10.0.0",
    "compression-webpack-plugin": "^11.1.0",
    "concurrently": "^8.0.1",
    "electron": "^27.0.0",
    "electron-builder": "^25.0.0",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-header": "^3.1.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.37.5",
    "html-webpack-plugin": "^5.6.3",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.5.0",
    "jest-junit": "^16.0.0",
    "lint-staged": "^13.0.3",
    "nodemon": "^2.0.19",
    "prettier": "^2.7.1",
    "react-test-renderer": "^17.0.2",
    "terser-webpack-plugin": "^5.3.14",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-unused-exports": "^9.0.4",
    "undici": "^7.9.0",
    "wait-on": "^7.0.1",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.1"
  },
  "dependencies": {
    "@elastic/datemath": "^5.0.3",
    "@elastic/eui": "^102.1.0",
    "@elastic/synthetics": "=1.18.0",
    "@emotion/cache": "^11.9.3",
    "@emotion/react": "^11.9.3",
    "archiver-utils": "^5.0.2",
    "color-name": "^2.0.0",
    "dotenv": "^16.0.3",
    "electron-debug": "^3.2.0",
    "electron-is-dev": "^2.0.0",
    "electron-log": "^4.4.8",
    "electron-unhandled": "^4.0.1",
    "playwright": "https://gitpkg.now.sh/elastic/playwright/packages/playwright?1.48.2-recorder",
    "playwright-chromium": "https://gitpkg.now.sh/elastic/playwright/packages/playwright-chromium?1.48.2-recorder",
    "playwright-core": "https://gitpkg.now.sh/elastic/playwright/packages/playwright-core?1.48.2-recorder",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "typescript": "^5.7.3"
  },
  "browserslist": [
    "last 1 version"
  ],
  "engines": {
    "node": ">=20.18.1",
    "npm": ">=7"
  }
}
